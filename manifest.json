
{
    "manifest_version": 3,
    "name": "Get Git",
    "version": "1.0",
    "description": "Never forget how to git. *Not sponsored by Matt*",
    "content_scripts": [
        {
            "matches": ["https://github.com/"],
            "js": ["./main.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    }
}
